<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="InterfaceName" value="Local Area Connection" />
    <add key="SmtpUsername" value="will-protect-your-string-on-first-execute" />
    <add key="SmtpPassword" value="will-protect-your-string-on-first-execute" />
    <add key="EmailRecipient" value="me@example.com" />
    <add key="EmailSubject" value="IP Address Changed" />
  </appSettings>
  <system.net>
    <mailSettings>
      <smtp from="me@example.com">
        <network host="smtp.example.com" port="465" enableSsl="true"/>
      </smtp>
    </mailSettings>
  </system.net>
</configuration>